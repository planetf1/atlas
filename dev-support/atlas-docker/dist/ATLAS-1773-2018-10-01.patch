+
+
+        try
+        {
+            repositoryValidator.validateClassificationProperties(repositoryName,
+                    classificationName,
+                    propertiesParameterName,
+                    properties,
+                    methodName);
+        }
+        catch (PropertyErrorException  error)
+        {
+            throw error;
+        }
